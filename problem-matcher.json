{
  "problemMatcher": [
    {
      "owner": "lilypond",
      "pattern": [
        {
          "regexp": "^(.+\\.i?ly):(?:(\\d+)(?::(\\d+))?:)? (?:(warning|error): )?(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
